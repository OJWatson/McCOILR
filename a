function findPositionX(obj){
var left = 0;
if(obj.offsetParent){
while(1){
left += obj.offsetLeft;
if(!obj.offsetParent)
break;
obj = obj.offsetParent;
}
}else if(obj.x){
left += obj.x;
}
return left;
}
function hideMe(obj){
document.getElementById(childMenu).style.display = 'none';
}
