function checkSearchOptions(){
$('#widgets-area').hide();
var searchPhrase = document.searchForm.inputSearch.value;
if (searchPhrase == 'Search' || searchPhrase == ''){
alert('Please add a topic before searching.');
}else{
/*Add this if they decide they want the drop down
if (document.searchForm.searchType.value == 'site'){
customSearchControl.execute(searchPhrase);
}else{
alert('send');
}*/
customSearchControl.execute(searchPhrase);
document.getElementById("cse").style.display = 'block';
document.getElementById("cse").style.height = '100%';
// document.getElementById("content").style.display = 'none';
setTimeout(function(){
smoothScroll(document.getElementById("cse"));
}, 750);
}
}
